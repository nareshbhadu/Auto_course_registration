{
    "name": "DTU Automatic Subject Register",
    "version": "1.0.0",
    "description": "Extension to register subjects",
    "permissions": ["tabs","activeTab","scripting","storage"],
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_title": "this is the title",
        "default_icon": {}
    },
    "icons": {
        "128": "logo.png"
    },
    "background": {
        "service_worker": "background.js"
    }
}